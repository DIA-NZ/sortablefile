{
    "name": "bummzack/sortablefile",
    "description": "An extension for the SilverStripe 4 UploadField to enable sorting.",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "CMS", "sortable", "file"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "bummzack"
        }
    ],
    "require": {
        "silverstripe/asset-admin": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "Bummzack\\SortableFile\\": "src/"
        }
    }
}
